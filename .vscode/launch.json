{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python:Django - Ansible galaxy",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "integratedTerminal",
            "args": [
                "runserver",
                "--noreload",
                "8089"
            ],
            "env": {"DJANGO_SETTINGS_MODULE":"galaxy.settings.development"},
            "django": true,
            "justMyCode":false
        }
    ],
    "compounds": []
}